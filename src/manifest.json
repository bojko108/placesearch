{
    "manifest_version": 2,
    "name": "Google Maps Search",
    "description": "Search places directly in Google Maps",
    "version": "2.1",
    "permissions": [
        "tabs",
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "geolocate.js"
        ]
    },
    "icons": {
        "16": "icon.png"
    },
    "omnibox": {
        "keyword": "gm"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Geolocate"
    }
}