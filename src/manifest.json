{
    "name": "Google Maps Search",
    "version": "3.0",
    "description": "Search places directly in Google Maps",
    "manifest_version": 3,
    "permissions": ["activeTab", "contextMenus"],
    "host_permissions": ["http://*/", "https://*/"],
    "background": {
        "service_worker": "geolocate.js"
    },
    "omnibox": {
        "keyword": "gm"
    },
    "action": {
        "default_icon": "icon.png",
        "default_title": "Geolocate",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon_16px.png",
        "32": "icon_32px.png"
    }
}